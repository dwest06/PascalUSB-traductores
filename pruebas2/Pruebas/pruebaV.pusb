program
begin
  declare d, sum, True, False, b as int, int, int, int, bool

	sum := 0;
	True := 0;
	False := 0;

	for i in 0..10 do
	begin
		if i%2==0 then
		 begin
			d := 1;
			b := i<7
		 end
		else
		 begin
			d := 0;
			a := i+1
		 end
	end;

	for i in 0..10*-1 do
	begin
		if d == 0 then
		 begin
			sum := sum + a
		 end
		else if b == true then
		 begin
			True:=True+1
		 end
		else
		 begin
			False:=False+1
		 end
	end
end
