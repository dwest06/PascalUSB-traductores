program
begin
declare a, b as int, bool


	i := 0;
	sum := 0;
	True := 0;
	False := 0;

	while i<10 do
		begin
			a := i*2;
			b := i < 7;
			i:=i+1
		end;

	// Sumatoria de los TRUE y FALSE
	while i/=0 do
		begin
			sum := sum + a;
			if b == true do
				True:=True+1
			else
				False:=False+1;
			i:=i-1
		end

end
